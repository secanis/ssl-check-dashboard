{
	"name": "ssl-check-dashboard",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev:client": "rollup -c -w",
		"dev:server": "nest start --debug --watch",
		"build:client": "rollup -c",
		"build:server": "nest build",
		"start:client": "sirv .svelte --no-clear --single",
		"lint:js": "eslint --ext \".ts,.js\" --ignore-path .gitignore .",
		"lint": "npm run lint:js",
		"format": "prettier --write \"server/**/*.ts\" \"client/**/*.(js|ts)\"",
		"check:client": "svelte-check --tsconfig ./tsconfig.json"
	},
	"dependencies": {
		"@nestjs-addons/in-memory-db": "^3.0.3",
		"@nestjs/common": "^8.0.0",
		"@nestjs/config": "^1.0.0",
		"@nestjs/core": "^8.0.0",
		"@nestjs/platform-express": "^8.0.0",
		"@nestjs/platform-socket.io": "^8.0.0",
		"@nestjs/serve-static": "^2.2.2",
		"@nestjs/websockets": "^8.0.0",
		"@types/socket.io": "^3.0.2",
		"autoprefixer": "^9.8.6",
		"core-js": "^3.15.1",
		"cron": "^1.8.2",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.2.0",
		"ssl-checker": "^2.0.7",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.14"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.14.7",
		"@nestjs/cli": "^8.0.2",
		"@nestjs/schematics": "^8.0.0",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.0",
		"@rollup/plugin-typescript": "^8.0.0",
		"@tsconfig/svelte": "^2.0.0",
		"autoprefixer": "^10.3.4",
		"cssnano": "^5.0.8",
		"eslint": "^7.29.0",
		"eslint-config-prettier": "^8.3.0",
		"postcss": "^8.3.6",
		"postcss-load-config": "^3.1.0",
		"prettier": "^2.3.2",
		"rollup": "^2.3.4",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-livereload": "^2.0.0",
		"rollup-plugin-svelte": "^7.0.0",
		"rollup-plugin-terser": "^7.0.0",
		"sirv-cli": "^1.0.0",
		"socket.io-client": "^4.2.0",
		"svelte": "^3.0.0",
		"svelte-check": "^2.0.0",
		"svelte-preprocess": "^4.9.4",
		"tailwindcss": "^2.2.15",
		"tslib": "^2.0.0",
		"typescript": "4.3.5"
	}
}
